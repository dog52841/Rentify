{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["es2020", "dom"],
    "moduleResolution": "node",
    "types": ["@supabase/supabase-js"],
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "allowJs": true,
    "checkJs": false,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "paths": {
      "std/*": ["./node_modules/@types/std/*"],
      "../_shared/*": ["./_shared/*"]
    }
  }
} 